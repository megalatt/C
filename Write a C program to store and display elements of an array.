#include<stdio.h>
int main()    
{    
    int arr[] = {1, 2, 3, 4, 5},n,i;
	printf("Enter size: ");
	scanf("%d",&n);
	printf("Enter elements: ");
	for(i=0;i<=n;i++){
	scanf("%d",&arr[i]);}
    printf("The elements of array: ");    
    for (int i = 0; i < n; i++) {
        printf("%d ", arr[i]);     
    }
    return 0;
}
