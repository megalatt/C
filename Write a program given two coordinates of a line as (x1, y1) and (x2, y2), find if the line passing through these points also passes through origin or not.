#include<stdio.h>
int main()
{
	int x1,y1,x2,y2;
	printf("Enter line coordinates of (x1, y1) : ");
	scanf("%d %d",&x1,&y1);
	printf("Enter line coordinates of (x2, y2) : ");
	scanf("%d %d",&x2,&y2);

if((x1 * (y2 - y1) == y1 * (x2 - x1)))
{
	printf("YES\n");
}
	else{
		printf("NO\n");
	}
	return 0;
}
