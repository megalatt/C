#include<stdio.h>
int main()
{
	int n,fib[10],i;
	printf("Enter a number : ");
	scanf("%d",&n);
	if(n==0)
	{
		printf("Invalid input\n");
	}
	else{
if(n>=0)
        fib[0] = 0;
      if(n>=1)
        fib[1] = 1;
      for(int i=2;i<n;i++)
        fib[i] = fib[i-1] + fib[i-2];
      for(int i=n-1;i>=0;i--)
		  {
		  printf("%d  ",fib[i]);
}}
printf("\n");
}
